@model AdminLTE9.Models.RequestedFlightInformation
@{
    ViewBag.Title = "آژنس هواپیمایی";
    ViewBag.Description = "Home Page";
}

@using (Html.BeginForm("SearchFlights", "Home", FormMethod.Post))
{
    <div style="padding-top:200px; padding-bottom:350px;">
        <div class="row" style="background-color: rgba(0, 4, 18, 0.74); width:1000px; height:500px; margin:0 auto; padding-top:30px;">

            <div class="row col-xs-12 col-md-12" style="padding-top:40px; padding-bottom:40px; padding-left:40px; padding-right:60px">
                <div class="col-xs-12">
                    <div class="form-group col-xs-6">
                        <label style="color:aliceblue; font-size:large;">مبدا پرواز</label>
                        <select class="form-control input-lg" name="Source" style="font-size:larger;" id="Source">
                            <option>---انتخاب کنید---</option>
                            <optgroup label="شهرهای داخلی">
                                @if (ViewBag.InternalCity != null)
                                {
                                    foreach (var InternalCity in ViewBag.InternalCity)
                                    {
                                        <option>@InternalCity.IC_Name</option>
                                    }
                                }
                            </optgroup>
                            <optgroup label="شهرهای خارجی">
                                @if (ViewBag.ExternalCity != null)
                                {
                                    foreach (var ExternalCity in ViewBag.ExternalCity)
                                    {
                                        <option>@ExternalCity.EC_Name</option>
                                    }
                                }
                            </optgroup>
                        </select>
                    </div>

                    <div class="form-group col-xs-6">
                        <label style="color:aliceblue; font-size:large">تاریخ پرواز</label>
                        <div class="input-group date input-group-lg">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                            @*<input type='text' class="form-control datepicker" id='Date' />*@
                            @*@Html.TextBoxFor(model => model.Date, new { @class = "form-control datepicker", placeholder = "Enter Drop-off date here...", id = "Date" })*@
                            <input class="form-control pull-right datepicker" data-date-format="yyyy/mm/dd" name="Date" id="Date">
                        </div>
                    </div>
                </div>
                <div class="col-xs-12">
                    <div class="form-group col-xs-6">
                        <label style="color:aliceblue; font-size:large">مقصد پرواز</label>
                        <select class="form-control input-lg" name="Destination" id="Destination" style="font-size:larger">
                            <option>---انتخاب کنید---</option>
                            <optgroup label="شهرهای داخلی">
                                @if (ViewBag.InternalCity != null)
                                {
                                    foreach (var InternalCity in ViewBag.InternalCity)
                                    {
                                        <option>@InternalCity.IC_Name</option>
                                    }
                                }
                            </optgroup>
                            <optgroup label="شهرهای خارجی">
                                @if (ViewBag.ExternalCity != null)
                                {
                                    foreach (var ExternalCity in ViewBag.ExternalCity)
                                    {
                                        <option>@ExternalCity.EC_Name</option>
                                    }
                                }
                            </optgroup>
                        </select>
                    </div>
                    <div class="form-group col-xs-6">
                        <label style="color:aliceblue; font-size:large">کلاس پروازی</label>
                        <select class="form-control input-lg" name="Class" id="Class" style="font-size:larger">
                            <option>---انتخاب کنید---</option>
                            @if (ViewBag.FlightClass != null)
                            {
                                foreach (var Class in ViewBag.FlightClass)
                                {
                                    <option>@Class.FC_Name</option>
                                }
                            }
                        </select>
                    </div>
                </div>
                <div class="form-group col-xs-12">
                    <div class="form-group col-xs-4">
                        <label style="font-size:large; color:aliceblue">تعداد بزرگسال - بالای 12 سال</label>
                        <input type="number" class="form-control input-lg" min="0" value="0" name="AdultNumber" id="AdultNumber" placeholder="بالای 12 سال">
                    </div>
                    <div class="form-group col-xs-4">
                        <label style="font-size:large; color:aliceblue">تعداد کودک - بین 2 تا 12 سال</label>
                        <input type="number" class="form-control input-lg" min="0" value="0" name="KidNumber" id="KidNumber" placeholder="بین 2 تا 12 سال" />

                    </div>
                    <div class="form-group col-xs-4">
                        <label style="font-size:large; color:aliceblue">تعداد نوزاد - کمتر از 2 سال</label>
                        <input type="number" class="form-control input-lg" min="0" value="0" name="LarvaNumber" id="LarvaNumber" placeholder="کمتر از 2 سال" />
                    </div>
                </div>
            </div>
            <div style="padding-left:350px; padding-right:350px">
                <input class="btn btn-primary btn-block btn-success btn-lg" type="submit" value="جستجوی پرواز" style="width:300px;">
            </div>
        </div>
    </div>
}
